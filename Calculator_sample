#include<stdio.h>
#include<conio.h>
#include<stdlib.h>

/*
type definition
	begin:			'<'
	numbers:		'1'
	end:			'>'
	else:			type = sprite
*/


typedef struct data{
	int *to;
	int value;
	int type;
	char sprite;
}data;
data node[100];

int input();
void assign(int i);

int main()
{
	int i,N;
	N=input();
	for(i=0;i<N;i++)
	printf(" %d",node[i].type);
}

int input()
{
	int i=1;
	do
	{
		scanf("%c",&node[i].sprite);
		assign(i);
		i++;
	}while(node[i-1].sprite!='\n');
	int N=i;
	
	return N;
}

void assign(int i)
{
	node[0].value=0;
	node[0].type='<';
	
	if(48<=node[i].sprite && node[i].sprite<=57)
	{
		node[i].value = node[i].sprite - 48;
		node[i].type = '1';
	}
	
	else if(node[i].sprite=='\n')
	{
		node[i].value = 0;
		node[i].type = '>';
	}
	
	else
	{
		node[i].value = 0;
		node[i].type = node[i].sprite;
	}
	
	node[i-1].to=&node[i].value;
}
